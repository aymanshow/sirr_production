      <blockAlignment value="LEFT"/>
      <!--blockValign value="CENTER"/-->
      <lineStyle kind="GRID" colorName="#e6e6e6"/>
      <!--lineStyle kind="LINEBELOW" colorName="#000000" start="0,0" stop="-1,0"/>
