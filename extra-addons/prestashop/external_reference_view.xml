        <record id="view_prestashop_log_tree" model="ir.ui.view">
            <field name="name">prestashop.log.tree</field>
            <field name="model">prestashop.log</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
            <tree string="Log">
                <field name="create_date"/>
                <field name="user_id"/>
                <field name="shop_id"/>
                <field name="state"/>
                <field name="res_model"/>
                <field name="sync_type"/>
                <field name="msg"/>
            </tree>
            </field>
        </record>
        
        <record id="view_prestashop_log_form" model="ir.ui.view">
            <field name="name">prestashop.log.form</field>
            <field name="model">prestashop.log</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
            <form string="Log">
                <field name="create_date" select="1"/>
                <field name="user_id" select="1"/>
                <field name="shop_id" select="1"/>
                <field name="state" select="1"/>
                <field name="res_model" select="1"/>
                <field name="sync_type" select="1"/>
                <field name="msg" select="1"/>
            </form>
            </field>
        </record>
        
        <record id="view_prestashop_log_search" model="ir.ui.view">
            <field name="name">prestashop.log.search</field>
            <field name="model">prestashop.log</field>
            <field name="type">search</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
            <search string="Log">
                <filter  string="Info" domain="[('state','=','info')]"/>
                <filter  string="Error" domain="[('state','=','error')]"/>
                <separator orientation="vertical"/>
                <filter  string="Import" domain="[('sync_type','=','import')]"/>
                <filter  string="Export" domain="[('sync_type','=','export')]"/>
                <separator orientation="vertical"/>
                <newline/>
                <field name="create_date" select="1"/>
                <field name="user_id" select="1"/>
                <field name="shop_id" select="1"/>
                <field name="state" select="1"/>
                <field name="res_model" select="1"/>
                <field name="sync_type" select="1"/>
                <field name="msg" select="1"/>
                <newline/>
                <group expand="0" string="Group By..." colspan="4" col="10" groups="base.group_extended">
                    <filter string="Sync Type"  domain="[]" context="{'group_by':'sync_type'}"/>
                    <filter string="State"  domain="[]" context="{'group_by':'state'}"/>
                    <filter string="Users"  domain="[]" context="{'group_by':'user_id'}"/>
                    <filter string="Shops"  domain="[]" context="{'group_by':'shop_id'}"/>
                    <filter string="Model"  domain="[]" context="{'group_by':'res_model'}"/>
                </group>
            </search>
            </field>
        </record>
        
        <record model="ir.actions.act_window" id="act_open_prestashop_log_view">
