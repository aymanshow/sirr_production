        <record id="view_external_reference_tree" model="ir.ui.view">
            <field name="name">external.reference.tree</field>
            <field name="model">external.reference</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
            <tree string="External Reference">
                <field name="res_model"/>
                <field name="res_id"/>
                <field name="ext_id"/>
                <field name="external_reference_id"/>
            </tree>
            </field>
        </record>
        
        <record id="view_external_reference_form" model="ir.ui.view">
            <field name="name">external.reference.form</field>
            <field name="model">external.reference</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
            <form string="External Reference">
                <field name="res_model" select="1"/>
                <field name="res_id" select="1"/>
                <field name="ext_id" select="1"/>
                <field name="external_reference_id" select="1"/>
            </form>
            </field>
        </record>
        
        <record id="view_external_reference_search" model="ir.ui.view">
            <field name="name">external.reference.search</field>
            <field name="model">external.reference</field>
            <field name="type">search</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
            <search string="External Reference">
                <field name="res_model" select="True"/>
                <field name="res_id" select="True"/>
                <field name="ext_id" select="True"/>
                <field name="external_reference_id" select="True"/>
            </search>
            </field>
        </record>
        
        <record model="ir.actions.act_window" id="act_open_external_reference_view">
