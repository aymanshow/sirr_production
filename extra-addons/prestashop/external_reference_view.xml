            <field name="type">ir.actions.act_window</field>
            <field name="res_model">external.reference</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_external_reference_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
        
        <record model="ir.actions.act_window.view" id="act_open_external_reference_view_form">
