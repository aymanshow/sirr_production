            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_prestashop_log_form"/>
        </record>
        
        <record model="ir.actions.act_window.view" id="act_open_prestashop_log_view_tree">
